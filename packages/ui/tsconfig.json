{
    "compilerOptions": {
        "target": "ESNext",
        "module": "node16",
        "moduleResolution": "node16",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "ignoreDeprecations": "6.0",
        "baseUrl": "./",
        "paths": {}
    },
    "include": [
        "index.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}